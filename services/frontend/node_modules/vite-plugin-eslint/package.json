{
  "name": "vite-plugin-eslint",
  "version": "1.6.1",
  "description": "ESLint plugin for vite.",
  "author": "Xiang Gao",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch ./src",
    "lint": "eslint ./src --ext .js,.ts",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "keywords": [
    "eslint",
    "vite-plugin"
  ],
  "homepage": "https://github.com/gxmari007/vite-plugin-eslint",
  "repository": "git+https://github.com:gxmari007/vite-plugin-eslint.git",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@types/eslint": "^8.4.2",
    "rollup": "^2.70.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "peerDependencies": {
    "eslint": ">=7",
    "vite": "^2.0.0"
  },
  "pnpm": {
    "overrides": {
      "vite-plugin-eslint": "workspace:*",
      "vue": "^3.2.33",
      "@vitejs/plugin-vue": "^2.3.1",
      "eslint-plugin-vue": "^8.7.0"
    }
  }
}
